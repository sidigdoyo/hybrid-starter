{
  "name": "starter-apps",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean:public": "rimraf public",
    "prestart": "npm run clean:public",
    "start": "gulp",
    "predeploy": "npm run clean:public",
    "deploy": "gulp deploy",
    "generate:apps": "rimraf build/www && npm run deploy && gulp copyPublic",
